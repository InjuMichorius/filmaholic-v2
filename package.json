{
  "name": "filmaholic",
  "version": "1.0.0",
  "description": "Filmaholic is a movie rating application that rates the suitability of movies to its audience using an API",
  "main": "app.js",
  "type": "module",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "predev": "npm run build:js & npm run build:css",
    "build:js": "node scripts/buildjs.js",
    "build:css": "node scripts/buildcss.js",
    "dev": "nodemon app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InjuMichorius/filmaholic-v2.git"
  },
  "keywords": [
